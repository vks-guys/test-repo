@*@model IEnumerable<AspCoreDemoProject.Models.Student>*@
@inject AspCoreDemoProject.Models.IStudentRepository _IStudentRepository
@{
    ViewData["Title"] = "Index";
    var data = _IStudentRepository.GetStudent();
}

<h1>Index</h1>

<table>
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Age</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in data)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
            </tr>
        }
    </tbody>
</table>
